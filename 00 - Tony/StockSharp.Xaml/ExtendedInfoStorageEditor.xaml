<dxe:ComboBoxEditSettings
    x:Class="StockSharp.Xaml.PropertyGrid.ExtendedInfoStorageEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:loc="clr-namespace:StockSharp.Localization;assembly=StockSharp.Localization"
    xmlns:local="clr-namespace:StockSharp.Xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    AutoComplete="true"
    DisplayMember="StorageName"
    IsTextEditable="False">

    <dxe:ButtonEditSettings.Buttons>
        <dxe:ButtonInfo
            x:Name="CancelBtn"
            Click="CancelBtn_Click"
            GlyphKind="Cancel"
            ToolTipService.ToolTip="{x:Static loc:LocalizedStrings.Cancel}" />
    </dxe:ButtonEditSettings.Buttons>

</dxe:ComboBoxEditSettings>
