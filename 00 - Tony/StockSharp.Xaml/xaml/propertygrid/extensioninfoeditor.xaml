<dxe:ButtonEditSettings
    x:Class="StockSharp.Xaml.PropertyGrid.ExtensionInfoEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:loc="clr-namespace:StockSharp.Localization;assembly=StockSharp.Localization"
    xmlns:local="clr-namespace:StockSharp.Xaml.PropertyGrid"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <dxe:ButtonEditSettings.Buttons>
        <dxe:ButtonInfo
            x:Name="EditBtn"
            Click="EditBtn_Click"
            GlyphKind="Regular"
            ToolTipService.ToolTip="{x:Static loc:LocalizedStrings.XamlStr607}" />
    </dxe:ButtonEditSettings.Buttons>
</dxe:ButtonEditSettings>
